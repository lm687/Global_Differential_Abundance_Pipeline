wrapper_run_ttest = function(i){
x = readRDS(i)
x = x[[1]]@count_matrices_all
props = sapply(x, normalise_rw, simplify = FALSE)
return(Compositional::hotel2T2(x1 = compositions::ilr(props[[1]]), x2 = compositions::ilr(props[[2]]))$pvalue)
}
